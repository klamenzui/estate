<%
if(includes) {
    let js={};
    let css={};
    let curIncludes = includes['top'] ? includes['top']: includes['bottom'];
    includes['top'] = false;
    if(curIncludes.js){
        js=curIncludes.js;
    }
    if(curIncludes.js){
        css=curIncludes.css;
    }
    for(let i in js){
        %><script src="<%=js[i];%>"></script><%
    }
    for(let i in css){%>
        <link rel="stylesheet" href="<%=css[i];%>">
    <% }
}
%>